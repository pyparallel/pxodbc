
// This was a Microsoft Visual C++ generated resource script, but I've hand modified it to
// remove the afxres.h include.  Apparently Visual Studio 2008 Express (used to build the
// Python 2.6 version) does not include afxres.h which is part of MFC.  This will probably
// not be editable in the Visual Studio resource editor.

#include <winres.h>
#include "resource.h"

#define STRINGIZER(version) #version
#define PRODUCT_VERSION_STRING(major,minor) STRINGIZER(major) "." STRINGIZER(minor)
#define FILE_VERSION_STRING(major,minor,micro,build) STRINGIZER(major) "." STRINGIZER(minor) "." STRINGIZER(micro) "." STRINGIZER(build)

LANGUAGE LANG_ENGLISH, SUBLANG_ENGLISH_US
#pragma code_page(1252)

// This should be picked up by our vcprojs file, but it's not.  Need to
// investigate.
#define PXODBC_MAJOR 3
#define PXODBC_MINOR 0
#define PXODBC_MICRO 8
#define PXODBC_BUILD 0

VS_VERSION_INFO VERSIONINFO
 FILEVERSION PXODBC_MAJOR,PXODBC_MINOR,PXODBC_MICRO,PXODBC_BUILD
 PRODUCTVERSION PXODBC_MAJOR,PXODBC_MINOR,PXODBC_MICRO,PXODBC_BUILD
 FILEFLAGSMASK 0x17L
#ifdef _DEBUG
 FILEFLAGS 0x1L
#else
 FILEFLAGS 0x0L
#endif
 FILEOS 0x4L
 FILETYPE 0x2L
 FILESUBTYPE 0x0L
BEGIN
    BLOCK "StringFileInfo"
    BEGIN
        BLOCK "040904b0"
        BEGIN
            VALUE "Copyright",        "Copyright 2009 Michael Kleehammer"
            VALUE "ProductName",      "ODBC DB API 2.0 Module (PyParallel)"
            VALUE "ProductVersion",   PRODUCT_VERSION_STRING(PXODBC_MAJOR,PXODBC_MINOR)
            VALUE "FileDescription",  "ODBC DB API 2.0 Module (PyParallel)"
            VALUE "FileVersion",      FILE_VERSION_STRING(PXODBC_MAJOR,PXODBC_MINOR,PXODBC_MICRO,PXODBC_BUILD)
            VALUE "InternalName",     "pxodbc"
            VALUE "OriginalFilename", "pxodbc.pyd"
        END
    END
    BLOCK "VarFileInfo"
    BEGIN
        VALUE "Translation", 0x409, 1200
    END
END
